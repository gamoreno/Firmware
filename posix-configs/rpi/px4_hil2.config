uorb start
param load
param set SYS_AUTOSTART 1001
param set MAV_BROADCAST 0
param set MAV_TYPE 2
param set SYS_MC_EST_GROUP 2
param set MAV_USEHILGPS 0

param set BAT_N_CELLS 3

#### from rc.mc_defaults
param set NAV_ACC_RAD 2.0
param set RTL_RETURN_ALT 30.0
param set RTL_DESCEND_ALT 10.0
param set PWM_MIN 1075
param set PWM_MAX 1950
param set PWM_RATE 400

param set RTL_LAND_DELAY 0

# use joystick 1 or 0 for RC
param set COM_RC_IN_MODE 0
sleep 1
#navio_rgbled start
sensors start -h
commander start --hil

sleep 1
mavlink start -d /dev/ttyAMA0 -b 921600

ekf2 start

mc_att_control start
mc_pos_control start
land_detector start multicopter

navio_sysfs_rc_in start
#navio_sysfs_pwm_out start
sleep 1
pwm_out_sim mode_pwm
#pwm_out_sim mode_pwm16
mixer load /dev/pwm_output0 ROMFS/px4fmu_common/mixers/quad_x.main.mix
navigator start
logger start -t
mavlink boot_complete
